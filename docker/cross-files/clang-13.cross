[binaries]
c = '/usr/bin/clang-13'
cpp = '/usr/bin/clang++-13'
ar = '/usr/bin/llvm-ar-13'
strip = '/usr/bin/llvm-strip-13'
objcopy = '/usr/bin/llvm-objcopy-13'
ld = '/usr/bin/llvm-ld-13'

[properties]
c_args = ['-march=native','-Weverything', '-fno-lax-vector-conversions', '-Werror']
cpp_args = ['-march=native', '-Weverything', '-fno-lax-vector-conversions', '-Werror']
