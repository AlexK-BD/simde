[binaries]
c = '/usr/bin/clang-15'
cpp = '/usr/bin/clang++-15'
ar = '/usr/bin/llvm-ar-15'
strip = '/usr/bin/llvm-strip-15'
objcopy = '/usr/bin/llvm-objcopy-15'
ld = '/usr/bin/llvm-ld-15'

[properties]
c_args = ['-march=native','-Weverything', '-fno-lax-vector-conversions', '-Werror']
cpp_args = ['-march=native', '-Weverything', '-fno-lax-vector-conversions', '-Werror']
