[binaries]
c = '/usr/bin/clang-12'
cpp = '/usr/bin/clang++-12'
ar = '/usr/bin/llvm-ar-12'
strip = '/usr/bin/llvm-strip-12'
objcopy = '/usr/bin/llvm-objcopy-12'
ld = '/usr/bin/llvm-ld-12'

[properties]
c_args = ['-march=native','-Weverything', '-fno-lax-vector-conversions', '-Werror']
cpp_args = ['-march=native', '-Weverything', '-fno-lax-vector-conversions', '-Werror']
