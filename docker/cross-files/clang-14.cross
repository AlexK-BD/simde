[binaries]
c = '/usr/bin/clang-14'
cpp = '/usr/bin/clang++-14'
ar = '/usr/bin/llvm-ar-14'
strip = '/usr/bin/llvm-strip-14'
objcopy = '/usr/bin/llvm-objcopy-14'
ld = '/usr/bin/llvm-ld-14'

[properties]
c_args = ['-march=native','-Weverything', '-fno-lax-vector-conversions', '-Werror']
cpp_args = ['-march=native', '-Weverything', '-fno-lax-vector-conversions', '-Werror']
